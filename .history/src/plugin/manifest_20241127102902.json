{
    "manifest_version": 3,
    "name": "my-vue3-plugin",
    "description": "基于vue3.x版本的chrome插件",
    "version": "1.0.0",
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "bookmarks"
    ],
    "background": {
        "scripts": [
            "js/chunk-vendors.js",
            "js/background.js"
        ],
        "persistent": false
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
}